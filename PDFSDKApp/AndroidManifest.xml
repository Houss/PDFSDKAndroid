<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.allappsmobile.pdfsdk.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/Theme.Sherlock.Light" >
        <activity
            android:name="com.allappsmobile.pdfsdk.app.ActivityDirectory"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity  android:name="com.allappsmobile.pdfsdk.app.PDFViewer" android:label="@string/app_name" ></activity>
        <activity  android:name="com.allappsmobile.pdfsdk.app.ActivityTOC" android:label="@string/app_name" ></activity>
        <activity  android:name="com.allappsmobile.pdfsdk.app.ActivityThumbs" android:label="@string/app_name" ></activity>
        <activity  android:name="com.allappsmobile.pdfsdk.app.ActivityBookmarks" android:label="@string/app_name" ></activity>
        <activity  android:name="com.allappsmobile.pdfsdk.app.PDFPageCurl" android:label="@string/app_name" ></activity>
    </application>
    
        

</manifest>
